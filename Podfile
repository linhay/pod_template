# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
project 'Template.xcodeproj'

target 'iOS' do
  use_frameworks!
  pod 'Template', :path => './'
end

target 'macOS' do
  use_frameworks!
  pod 'Template', :path => './'
end

target 'tvOS' do
  use_frameworks!
  pod 'Template', :path => './'
end
